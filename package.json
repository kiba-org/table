{
  "name": "@kiba-org/table",
  "version": "1.0.0",
  "description": "Kiba-org  table",
  "main": "./build/index.js",
  "source": "./src/index.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": "./build/index.js",
    "./table": "./build/table"
  },
  "files": [
    "build/**/*.js",
    "build/**/*.d.ts",
    "README.md",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "build": "npm run clean && tsc --project tsconfig.build.json  --outDir ./build",
    "postbuild": "prettier --write \"build/**/*.js\" \"build/**/*.d.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"test/**/*.ts\" \"test/**/*.tsx\" \"*.md\" \"package.json\"",
    "prepublish": "npm run build",
    "prestart": "npm run build",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiba-org/table.git"
  },
  "keywords": [
    "kiba",
    "table",
    "react",
    "table"
  ],
  "author": "Hakim Gueye <beatsjordy@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kiba-org/table/issues"
  },
  "homepage": "https://github.com/kiba-org/table#readme",
  "engines": {
    "node": ">=18"
  },
  "prettier": {
    "semi": false,
    "useTabs": true,
    "singleQuote": true,
    "proseWrap": "always",
    "overrides": [
      {
        "files": [
          "**/*.json"
        ],
        "options": {
          "useTabs": false
        }
      }
    ]
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "@radix-ui/react-dialog": {
      "optional": true
    },
    "@radix-ui/react-label": {
      "optional": true
    },
    "@radix-ui/react-popover": {
      "optional": true
    },
    "@radix-ui/react-select": {
      "optional": true
    },
    "tailwindcss": {
      "optional": true
    },
    "tailwind-merge": {
      "optional": true
    },
    "class-variance-authority": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.34",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "happy-dom": "^12.10.3",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "type-fest": "^4.3.3"
  },
  "peerDependencies": {
    "@radix-ui/react-dialog": "*",
    "@radix-ui/react-label": "*",
    "@radix-ui/react-popover": "*",
    "@radix-ui/react-select": "*",
    "@radix-ui/react-separator": "*",
    "@tanstack/react-table": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "class-variance-authority": "*",
    "clsx": "*",
    "cmdk": "*",
    "react": "^18.0.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "*"
  }
}
